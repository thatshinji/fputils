{
  "name": "@evageeks/fputils",
  "version": "0.0.1",
  "description": "a functional-programming utils library",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.aio.js",
  "files": [
    "/dist",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thatshinji/fputils.git"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build:cjs": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:aio": "rollup -c config/rollup.config.aio.js",
    "build": "npm run build:cjs && npm run build:esm && npm run build:aio",
    "test": "cross-env NODE_ENV=test jest --coverage=true",
    "prepare": "husky install",
    "ci": "commit",
    "gz": "git-cz"
  },
  "keywords": [
    "functional-programming"
  ],
  "author": "thatshinji",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/cz-commitlint": "^17.3.0",
    "@commitlint/prompt-cli": "^17.3.0",
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-jest": "^29.3.1",
    "babel-plugin-istanbul": "^6.1.1",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "2.8.1",
    "pretty-quick": "^3.1.3",
    "rollup": "^3.7.5",
    "ts-jest": "^29.0.3"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.20.6",
    "@babel/runtime-corejs3": "^7.20.6",
    "core-js": "^3.26.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
